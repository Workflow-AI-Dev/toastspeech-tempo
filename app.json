{
  "expo": {
    "name": "Echozi",
    "slug": "test-tempo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/closeup.jpg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "Allow $(PRODUCT_NAME) to access your camera"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/closeup.jpg",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.INTERNET"
      ],
      "package": "com.mazar.testtempo"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/full.jpg",
          "imageWidth": 400,
          "resizeMode": "contain",
          "backgroundColor": "#925bd2"
        }
      ],
      "react-native-vision-camera",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "a9dfac04-d8f3-437f-bb58-66d9110505d7"
      }
    }
  }
}
